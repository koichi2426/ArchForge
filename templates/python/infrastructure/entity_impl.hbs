{% for imp in imports %}
from {{ imp.from_ }} import {{ imp.name }}
{% endfor %}

class {{ name }}Impl({{ name }}):
    def __init__(self{% for prop in properties %}, {{ prop.name }}: {{ prop.type }}{% endfor %}):
{% for prop in properties %}
        self.{{ prop.name }} = {{ prop.name }}
{% endfor %}

{% for method in methods %}
    {% if "Promise" in method.output %}
    async def {{ method.name }}(self{% for input in method.inputs %}, {{ input }}{% endfor %}) -> {{ method.output | replace("Promise<", "").replace(">", "") }}:
    {% else %}
    def {{ method.name }}(self{% for input in method.inputs %}, {{ input }}{% endfor %}) -> {{ method.output }}:
    {% endif %}
        # 各実装をお願いします。
        pass

{% endfor %}
